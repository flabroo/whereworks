    <%= form_with url: listings_path, method: :get do |f| %>
      <%= f.text_field :address, placeholder: "Location", class: "input-text" %>
      <%= f.text_field :address,
      input_html: {data: {address_autocomplete_target: "address"}},
      wrapper_html: {data: {controller: "address-autocomplete", address_autocomplete_api_key_value: ENV["MAPBOX_API_KEY"]}}
      %>
      <%= f.date_field :date, value: Date.today %>
      <%= f.time_field :start_time, value: time %>
      <%= f.time_field :end_time, value: newtime %>
      <%= f.submit "Search", class: "btn btn-banner" %>
      <% end %>
    <%= simple_form_for :listing, url: listings_path, method: :get do |f| %>
      <%= f.input :address,
        input_html: {data: {address_autocomplete_target: "address"}},
        wrapper_html: {data: {controller: "address-autocomplete", address_autocomplete_api_key_value: ENV["MAPBOX_API_KEY"]}}
      %>
      <%= f.button :submit, value: "Search", class: "btn btn-banner" %>
      <% end %>
